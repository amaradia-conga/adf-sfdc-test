{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-datasync-sfdc-connectivity"
		},
		"Salesforce6_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Salesforce6'"
		},
		"Salesforce6_securityToken": {
			"type": "secureString",
			"metadata": "Secure string for 'securityToken' of 'Salesforce6'"
		},
		"Salesforce_test_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Salesforce_test'"
		},
		"Salesforce_test_securityToken": {
			"type": "secureString",
			"metadata": "Secure string for 'securityToken' of 'Salesforce_test'"
		},
		"Salesforce6_properties_typeProperties_environmentUrl": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"value": "@{linkedService().salesforceUrl}"
			}
		},
		"Salesforce6_properties_typeProperties_username": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"value": "@{linkedService().userName}"
			}
		},
		"Salesforce_test_properties_typeProperties_environmentUrl": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"value": "@{linkedService().salesforceUrl}"
			}
		},
		"Salesforce_test_properties_typeProperties_username": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"value": "@{linkedService().userName}"
			}
		},
		"ls_salesforce_test_with_no_kv_properties_typeProperties_environmentUrl": {
			"type": "string",
			"defaultValue": "@{linkedService().salesforceUrl}"
		},
		"ls_salesforce_test_with_no_kv_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "@{linkedService().userName}"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "SalesforceSource",
								"query": "Select Id, Name From Account LIMIT 1",
								"readBehavior": "query"
							},
							"sink": {
								"type": "SalesforceSink",
								"writeBatchSize": 5000,
								"writeBehavior": "insert"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "test_data",
								"type": "DatasetReference",
								"parameters": {
									"un": {
										"value": "@pipeline().parameters.un",
										"type": "Expression"
									},
									"pass": {
										"value": "@pipeline().parameters.pass",
										"type": "Expression"
									},
									"token": {
										"value": "@pipeline().parameters.token",
										"type": "Expression"
									},
									"url": {
										"value": "@pipeline().parameters.url",
										"type": "Expression"
									},
									"apiv": {
										"value": "@pipeline().parameters.apiv",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
								"referenceName": "test_data",
								"type": "DatasetReference",
								"parameters": {
									"un": {
										"value": "@pipeline().parameters.un",
										"type": "Expression"
									},
									"pass": {
										"value": "@pipeline().parameters.pass",
										"type": "Expression"
									},
									"token": {
										"value": "@pipeline().parameters.token",
										"type": "Expression"
									},
									"url": {
										"value": "@pipeline().parameters.url",
										"type": "Expression"
									},
									"apiv": {
										"value": "@pipeline().parameters.apiv",
										"type": "Expression"
									}
								}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"parameters": {
					"un": {
						"type": "string"
					},
					"pass": {
						"type": "string"
					},
					"token": {
						"type": "string"
					},
					"url": {
						"type": "string"
					},
					"apiv": {
						"type": "string"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/test_data')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/test_data')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_salesforce_test_with_no_kv",
					"type": "LinkedServiceReference",
					"parameters": {
						"userName": {
							"value": "@dataset().un",
							"type": "Expression"
						},
						"apiVersion": {
							"value": "@dataset().apiv",
							"type": "Expression"
						},
						"password": {
							"value": "@dataset().pass",
							"type": "Expression"
						},
						"securityToken": {
							"value": "@dataset().token",
							"type": "Expression"
						},
						"salesforceUrl": {
							"value": "@dataset().url",
							"type": "Expression"
						}
					}
				},
				"parameters": {
					"un": {
						"type": "string"
					},
					"pass": {
						"type": "string"
					},
					"token": {
						"type": "string"
					},
					"url": {
						"type": "string"
					},
					"apiv": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "SalesforceObject",
				"schema": [],
				"typeProperties": {
					"objectApiName": "Account"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_salesforce_test_with_no_kv')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Salesforce6')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"userName": {
						"type": "String"
					},
					"apiVersion": {
						"type": "String"
					},
					"password": {
						"type": "SecureString"
					},
					"securityToken": {
						"type": "SecureString"
					},
					"salesforceUrl": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "Salesforce",
				"typeProperties": {
					"environmentUrl": "[parameters('Salesforce6_properties_typeProperties_environmentUrl')]",
					"username": "[parameters('Salesforce6_properties_typeProperties_username')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('Salesforce6_password')]"
					},
					"securityToken": {
						"type": "SecureString",
						"value": "[parameters('Salesforce6_securityToken')]"
					},
					"apiVersion": {
						"type": "String",
						"value": "@{linkedService().apiVersion}"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Salesforce_test')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"userName": {
						"type": "String"
					},
					"apiVersion": {
						"type": "String"
					},
					"password": {
						"type": "SecureString"
					},
					"securityToken": {
						"type": "SecureString"
					},
					"salesforceUrl": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "Salesforce",
				"typeProperties": {
					"environmentUrl": "[parameters('Salesforce_test_properties_typeProperties_environmentUrl')]",
					"username": "[parameters('Salesforce_test_properties_typeProperties_username')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('Salesforce_test_password')]"
					},
					"securityToken": {
						"type": "SecureString",
						"value": "[parameters('Salesforce_test_securityToken')]"
					},
					"apiVersion": "@{linkedService().apiVersion}"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_salesforce_test_with_no_kv')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"userName": {
						"type": "String"
					},
					"apiVersion": {
						"type": "String"
					},
					"password": {
						"type": "String"
					},
					"securityToken": {
						"type": "String"
					},
					"salesforceUrl": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "Salesforce",
				"typeProperties": {
					"environmentUrl": "[parameters('ls_salesforce_test_with_no_kv_properties_typeProperties_environmentUrl')]",
					"username": "[parameters('ls_salesforce_test_with_no_kv_properties_typeProperties_username')]",
					"password": "@{linkedService().password}",
					"securityToken": "@{linkedService().securityToken}",
					"apiVersion": "@{linkedService().apiVersion}"
				}
			},
			"dependsOn": []
		}
	]
}