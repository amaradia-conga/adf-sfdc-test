{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-datasync-sfdc-connectivity"
		},
		"Salesforce_test_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Salesforce_test'"
		},
		"Salesforce_test_securityToken": {
			"type": "secureString",
			"metadata": "Secure string for 'securityToken' of 'Salesforce_test'"
		},
		"Salesforce6_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Salesforce6'"
		},
		"Salesforce6_securityToken": {
			"type": "secureString",
			"metadata": "Secure string for 'securityToken' of 'Salesforce6'"
		},
		"Salesforce_test_properties_typeProperties_environmentUrl": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"value": "@{linkedService().salesforceUrl}"
			}
		},
		"Salesforce_test_properties_typeProperties_username": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"value": "@{linkedService().userName}"
			}
		},
		"Salesforce6_properties_typeProperties_environmentUrl": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"value": "@{linkedService().salesforceUrl}"
			}
		},
		"Salesforce6_properties_typeProperties_username": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"value": "@{linkedService().userName}"
			}
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Salesforce_test')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"userName": {
						"type": "String"
					},
					"apiVersion": {
						"type": "String"
					},
					"password": {
						"type": "SecureString"
					},
					"securityToken": {
						"type": "SecureString"
					},
					"salesforceUrl": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "Salesforce",
				"typeProperties": {
					"environmentUrl": "[parameters('Salesforce_test_properties_typeProperties_environmentUrl')]",
					"username": "[parameters('Salesforce_test_properties_typeProperties_username')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('Salesforce_test_password')]"
					},
					"securityToken": {
						"type": "SecureString",
						"value": "[parameters('Salesforce_test_securityToken')]"
					},
					"apiVersion": "@{linkedService().apiVersion}"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Salesforce6')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"userName": {
						"type": "String"
					},
					"apiVersion": {
						"type": "String"
					},
					"password": {
						"type": "SecureString"
					},
					"securityToken": {
						"type": "SecureString"
					},
					"salesforceUrl": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "Salesforce",
				"typeProperties": {
					"environmentUrl": "[parameters('Salesforce6_properties_typeProperties_environmentUrl')]",
					"username": "[parameters('Salesforce6_properties_typeProperties_username')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('Salesforce6_password')]"
					},
					"securityToken": {
						"type": "SecureString",
						"value": "[parameters('Salesforce6_securityToken')]"
					},
					"apiVersion": {
						"type": "String",
						"value": "@{linkedService().apiVersion}"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/test_data')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "Salesforce6",
					"type": "LinkedServiceReference",
					"parameters": {
						"userName": {
							"value": "@dataset().un",
							"type": "Expression"
						},
						"apiVersion": {
							"value": "@dataset().apiv",
							"type": "Expression"
						},
						"password": {
							"value": "@dataset().pass",
							"type": "Expression"
						},
						"securityToken": {
							"value": "@dataset().token",
							"type": "Expression"
						},
						"salesforceUrl": {
							"value": "@dataset().url",
							"type": "Expression"
						}
					}
				},
				"parameters": {
					"un": {
						"type": "string"
					},
					"pass": {
						"type": "securestring"
					},
					"token": {
						"type": "securestring"
					},
					"url": {
						"type": "string"
					},
					"apiv": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "SalesforceObject",
				"schema": [],
				"typeProperties": {}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/Salesforce6')]"
			]
		}
	]
}